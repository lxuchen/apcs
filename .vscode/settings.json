{
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.debug.settings.console": "integratedTerminal",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "[java]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true,
        "editor.formatOnSave": true
    },
    "[markdown]": {
        "editor.wordWrap": "on"
    },
    "workbench.editor.enablePreview": false
}